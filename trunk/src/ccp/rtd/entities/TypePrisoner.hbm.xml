<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="ccp.rtd.entities.TypePrisoner">
  	<id name="typePrisonerID" column="TypePrisonerID" type="java.lang.Integer" >
            <generator class="native" />
        </id>
        <property name="name" column="TypePrisonerName" type="string" length="150" not-null="true"/>
        <property name="subDesc" column="SubTypePrisonerDesc" type="string" length="200" not-null="true"/>
        
  </class>
</hibernate-mapping>